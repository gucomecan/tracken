{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.options": {
    "extentions": [".js", ".jsx", ".ts", ".tsx"]
  },
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"]
}